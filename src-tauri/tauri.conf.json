{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "bjj-scoreboard",
  "version": "0.1.0",
  "identifier": "com.nicosismonda.bjjscoreboard",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "BJJ Scoreboard",
        "visible": true,
        "center": true,
        "minWidth": 800,
        "minHeight": 600,
        "maximized": false,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon_32x32.png",
      "icons/icon_128x128.png",
      "icons/icon_256x256.png",
      "icons/icon_512x512.png"
    ],
    "windows": {
      "certificateThumbprint": "A43047B4735809F2D65DDFFACF2CEDE60823DF4A",
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com"
    }
  }
}
